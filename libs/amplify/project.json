{
  "sourceRoot": "libs/amplify/src",
  "projectType": "library",
  "targets": {
    "analyze": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter analyze",
        "cwd": "libs/amplify"
      }
    },
    "clean": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter clean",
        "cwd": "libs/amplify"
      }
    },
    "format": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter format libs/amplify/*",
        "cwd": "libs/amplify"
      }
    },
    "buildAar": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter build aar",
        "cwd": "libs/amplify"
      }
    },
    "buildApk": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter build apk",
        "cwd": "libs/amplify"
      }
    },
    "buildAppbundle": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter build appbundle",
        "cwd": "libs/amplify"
      }
    },
    "buildBundle": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter build bundle",
        "cwd": "libs/amplify"
      }
    },
    "buildIos": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter build ios",
        "cwd": "libs/amplify"
      }
    },
    "buildIosFramework": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter build ios-framework",
        "cwd": "libs/amplify"
      }
    },
    "buildIpa": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter build ipa",
        "cwd": "libs/amplify"
      }
    },
    "get": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter pub get",
        "cwd": "libs/amplify"
      }
    },
    "amppull": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "amplify pull",
        "cwd": "libs/amplify"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/amplify"],
      "options": {
        "jestConfig": "libs/amplify/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
