{
  "root": "apps/app",
  "sourceRoot": "apps/app/src",
  "projectType": "application",
  "targets": {
    "analyze": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter analyze",
        "cwd": "apps/app"
      }
    },
    "clean": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter clean",
        "cwd": "apps/app"
      }
    },
    "format": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter format apps/app/*",
        "cwd": "apps/app"
      }
    },
    "test": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter test --coverage",
        "cwd": "apps/app"
      }
    },
    "assemble": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter assemble",
        "cwd": "apps/app"
      }
    },
    "attach": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter attach",
        "cwd": "apps/app"
      }
    },
    "drive": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter drive",
        "cwd": "apps/app"
      }
    },
    "genL10n": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter gen-l10n",
        "cwd": "apps/app"
      }
    },
    "install": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter install",
        "cwd": "apps/app"
      }
    },
    "run": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter run",
        "cwd": "apps/app"
      }
    },
    "buildAar": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter build aar",
        "cwd": "apps/app"
      }
    },
    "buildApk": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter build apk",
        "cwd": "apps/app"
      }
    },
    "buildAppbundle": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter build appbundle",
        "cwd": "apps/app"
      }
    },
    "buildBundle": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter build bundle",
        "cwd": "apps/app"
      }
    },
    "buildIos": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter build ios",
        "cwd": "apps/app"
      }
    },
    "buildIosFramework": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter build ios-framework",
        "cwd": "apps/app"
      }
    },
    "buildIpa": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter build ipa",
        "cwd": "apps/app"
      }
    },
    "get": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "flutter pub get",
        "cwd": "apps/app"
      }
    }
  },
  "tags": []
}
